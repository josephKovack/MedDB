<p id="notice"><%= notice %></p>

<h1>Hospitals</h1>

<table>
  <thead>
    <tr>
      <th>Hospital</th>
      <th>Hospital name</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Telephone</th>
      <th>Hospital type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hospitals.each do |hospital| %>
      <tr>
        <td><%= hospital.hospital_id %></td>
        <td><%= hospital.hospital_name %></td>
        <td><%= hospital.address %></td>
        <td><%= hospital.city %></td>
        <td><%= hospital.state %></td>
        <td><%= hospital.telephone %></td>
        <td><%= hospital.hospital_type %></td>
        <td><%= link_to 'Show', hospital %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
